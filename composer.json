{
    "name": "dotfiles/bashit-plugin",
    "description": "Dotfiles Bash-It Plugin",
    "type": "dotfiles-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1 || ^7.2",
        "bash-it/bash-it": "^0.1.0"
    },
    "require-dev": {
        "dotfiles/core": "^1.0@dev",
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": { "Dotfiles\\Plugins\\BashIt\\": "./src" }
    },
    "autoload-dev": {
        "psr-4": { "Dotfiles\\Plugins\\BashIt\\Tests\\": "./tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
