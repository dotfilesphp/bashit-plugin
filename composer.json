{
    "name": "dotfiles/bashit-plugin",
    "description": "Dotfiles Bash-It Plugin",
    "type": "dotfiles-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "bash-it/bash-it",
                "version": "1.0.0",
                "dist": {
                    "url": "https://codeload.github.com/Bash-it/bash-it/zip/master",
                    "type": "zip"
                }
            }
        }
    ],
    "require-dev": {
        "dotfiles/bash-plugin": "^1.0@dev",
        "dotfiles/core": "^1.0@dev",
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": { "Dotfiles\\Plugins\\BashIt\\": "./src" }
    },
    "autoload-dev": {
        "psr-4": { "Dotfiles\\Plugins\\BashIt\\Tests\\": "./tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "bash-it/bash-it": "^1.0"
    }
}
